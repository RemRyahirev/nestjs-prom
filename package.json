{
  "name": "@remryahirev/nestjs-prom",
  "version": "1.0.0",
  "description": "A promotheus module for nestjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/ts-node tests/src/main.ts",
    "test:live": "ts-node tests/src/main.ts"
  },
  "author": "Andrey Ryahovskiy <remryahirev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/common": "^10.1.2",
    "@nestjs/core": "^10.1.2",
    "@nestjs/platform-express": "^10.1.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@nestjs/common": ">= 6.0.0",
    "@nestjs/core": ">= 6.0.0",
    "reflect-metadata": ">= 0.1.12",
    "rxjs": ">= 6.0.0"
  },
  "dependencies": {
    "@nestjs/testing": "^10.1.2",
    "prom-client": "^14.2.0",
    "response-time": "^2.3.2",
    "url-value-parser": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remryahirev/nestjs-prom.git"
  },
  "keywords": [
    "nestjs",
    "promotheus"
  ],
  "bugs": {
    "url": "https://github.com/remryahirev/nestjs-prom/issues"
  },
  "homepage": "https://github.com/remryahirev/nestjs-prom#readme",
  "contributors": [
    "Tieu-Philippe KHIM <pkhim@digikare.com>",
    "Michael Yankelev @FSM1",
    "Ashleigh Simonelli @bashleigh",
    "@blackkopcap",
    "@litichevskiydv"
  ],
  "publishConfig": {
    "tag": "next"
  }
}
